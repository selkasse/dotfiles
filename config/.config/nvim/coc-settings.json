{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "html"
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"]
}
